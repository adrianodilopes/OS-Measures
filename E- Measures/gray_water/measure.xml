      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>graywater_tank_name</name>
      <display_name>Graywater tank name:</display_name>
      <type>String</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>Graywater Tank</default_value>
    </argument>
    <argument>
      <name>graywater_tank_capacity</name>
      <display_name>graywater tank Maximum Capacity [m3]:</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.25</default_value>
    </argument>
    <argument>
      <name>graywater_tank_init_vol</name>
      <display_name>graywater tank Initial Volume [m3]:</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.2</default_value>
    </argument>
    <argument>
      <name>graywater_tank_in_flowrate</name>
      <display_name>graywater tank Design In Flow Rate [m3/s]:</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.2</default_value>
    </argument>
    <argument>
      <name>graywater_tank_out_flowrate</name>
      <display_name>graywater tank Design out Flow Rate [m3/s]:</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.01</default_value>
    </argument>
    <argument>
      <name>graywater_tank_supply_type</name>
      <display_name>graywater tank Type of Supply:</display_name>
      <type>Choice</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>Mains</default_value>
      <choices>
        <choice>
          <value>None</value>
          <display_name>None</display_name>
        </choice>
        <choice>
          <value>Mains</value>
          <display_name>Mains</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>float_valve_on_cap</name>
      <display_name>Float Valve On Capacity [m3]:</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.02</default_value>
    </argument>
    <argument>
      <name>float_valve_off_cap</name>
      <display_name>Float Valve Off Capacity [m3]:</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.135</default_value>
    </argument>
    <argument>
      <name>float_backup_mains_cap</name>
      <display_name>Backup Mains Capacity [m3]:</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.04</default_value>
    </argument>
    <argument>
      <name>tank_temp_schedule</name>
      <display_name>Tank Temperature schedule:</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>add_meters</name>
      <display_name>Add Meter:Custom and Output:Meter objects for the graywater tank?</display_name>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
