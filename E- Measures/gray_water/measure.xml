  <class_name>GrayWater</class_name>
  <display_name>GrayWater</display_name>
  <description>The measure adds gray water storage and let the user select source and sink water use connection for that storage.</description>
  <modeler_description>Gray water tank overflow will be dirrected to drainage. </modeler_description>
  <arguments>
    <argument>
      <name>sink_water_use_connections</name>
      <display_name>Water Use Connections to to be supplied with graywater:</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>source_water_use_connections</name>
      <display_name>Water Use Connections to be used for gray water reclamation:</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>graywater_heat_exchange_type</name>
      <display_name>Drain Water Heat Exchanger Type:</display_name>
      <type>Choice</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>CounterFlow</default_value>
      <choices>
        <choice>
          <value>None</value>
          <display_name>None</display_name>
        </choice>
        <choice>
          <value>Ideal</value>
          <display_name>Ideal</display_name>
        </choice>
        <choice>
          <value>CounterFlow</value>
          <display_name>CounterFlow</display_name>
        </choice>
        <choice>
          <value>CrossFlow</value>
          <display_name>CrossFlow</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>graywater_heat_exchange_dest</name>
      <display_name>Drain Water Heat Exchanger Destination:</display_name>
      <type>Choice</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>PlantAndEquipment</default_value>
      <choices>
        <choice>
          <value>Plant</value>
          <display_name>Plant</display_name>
        </choice>
        <choice>
          <value>Equipment</value>
          <display_name>Equipment</display_name>
        </choice>
        <choice>
          <value>PlantAndEquipment</value>
          <display_name>PlantAndEquipment</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>graywater_ua</name>
